# NOTE: The first traffic lane definition is used as default if not set otherwise (e.g. for unconnected prefabs or trajectories)

traffic_lane_data : traffic_lane.default
{
	speed_class: local_road

	rank: 0

	traffic_rules[]: traffic_rule.road_rail
}

