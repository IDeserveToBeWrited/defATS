traffic_trailer : traffic.trailer.gooseneck_r 
{
	accessories[]: .traffic.gooseneck_r.tchassis
	accessories[]: .traffic.gooseneck_r.trwheel0
	accessories[]: .traffic.gooseneck_r.trwheel1
	
	cargo_mass: 0

}

vehicle_accessory: .traffic.gooseneck_r.tchassis
{
    	data_path: "/def/vehicle/trailer/gooseneck/chassis_r.sii"
}

vehicle_wheel_accessory: .traffic.gooseneck_r.trwheel0
{
	offset: 0
    	data_path: "/def/vehicle/t_wheel/single.sii"
}

vehicle_wheel_accessory: .traffic.gooseneck_r.trwheel1
{
	offset: 2
    	data_path: "/def/vehicle/t_wheel/single.sii"
}


