	lod_dist[]: 150


	tags[]: metro

	spawn_condition[]: .traffic.oakland_loco.condition.city

}
vehicle_accessory: .oakland_loco.chassis
{
	data_path: "/def/vehicle/ai/oakland_metro/chassis_oakland_loco.sii"
}
