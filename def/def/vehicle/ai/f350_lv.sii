traffic_vehicle : traffic.f350_lv
{
	accessories[]: .f350_lv.chassis
	accessories[]: .f350_lv.fwheel
	accessories[]: .f350_lv.rwheel

	max_speed: 160

	lod_dist[]: 70
	lod_dist[]: 110
	allow_parked: false

	vehicle_mass: 2700

@include "drivers_generic.sui"
}

vehicle_accessory: .f350_lv.chassis
{
	data_path: "/def/vehicle/ai/f350/chassis_lv.sii"
}

vehicle_wheel_accessory: .f350_lv.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/front_box.sii"
}

vehicle_wheel_accessory: .f350_lv.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/rear_box.sii"
}

