	accessories[]: .larus.motorhome.fwheel
	accessories[]: .larus.motorhome.rwheel

	max_speed: 100
	vehicle_mass: 15000
	engine_power: 336

	sound_move[]: "/sound/traffic/truck_us_2.soundref"
	sound_horn[]: "/sound/traffic/truck_horn_3.soundref"

	tags[]: caravan
	
	variant[]: traffic.larus.motorhome.orange

	spawn_ratio: 1.0

	license_plate_type: car



@include "drivers_generic.sui"
}

