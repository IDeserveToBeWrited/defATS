traffic_trailer : train_car.cargo_d.dump
{
	accessories[]: .cargo_d.dump.tchassis
	accessories[]: .cargo_d.dump.trwheel0
	accessories[]: .cargo_d.dump.trwheel1
	accessories[]: .cargo_d.dump.trwheel2
	accessories[]: .cargo_d.dump.trwheel3

	attachable_trailer[]: "train_car.cargo_d.*"
	displacement_limit: 0.0
    
	lod_dist[]: 53
	lod_dist[]: 70
}

