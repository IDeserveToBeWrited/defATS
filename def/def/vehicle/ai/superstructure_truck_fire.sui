# XXXXXXXXXXXXXXXXXXX FIRE XXXXXXXXXXXXXXXXXXX

traffic_vehicle : traffic.strtruck.fire
{
	accessories[]: .strtruck.fire.chassis
	accessories[]: .strtruck.fire.fwheel
	accessories[]: .strtruck.fire.rwheel

	max_speed: 140

	sound_move[]: "/sound/traffic/car_v8_2.soundref"
	sound_horn[]: "/sound/traffic/car_horn_3.soundref"
	
	spawn_ratio: 0.03
	tags[]: rural

	variant[]: traffic.strtruck.land
	variant[]: traffic.strtruck.forest

	vehicle_mass: 3000


@include "drivers_generic.sui"
	
}

vehicle_accessory: .strtruck.fire.chassis
{
	data_path: "/def/vehicle/ai/superstructure_truck_fire/chassis_fire.sii"
}

