traffic_vehicle : traffic.sierra_hd
{
	accessories[]: .sierra_hd.chassis
	accessories[]: .sierra_hd.fwheel
	accessories[]: .sierra_hd.rwheel

	max_speed: 160

	sound_move[]: "/sound/traffic/car_v8_1.soundref"
	sound_move[]: "/sound/traffic/car_v8_2.soundref"
	sound_horn[]: "/sound/traffic/car_horn_2.soundref"
	vehicle_mass: 3100

	tags[]: pickup
	tags[]: empty
	tags[]: suv
	tags[]: common

@include "drivers_generic.sui"
}

vehicle_accessory: .sierra_hd.chassis
{
	data_path: "/def/vehicle/ai/sierra_hd/chassis.sii"
}

