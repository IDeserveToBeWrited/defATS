traffic_vehicle : traffic.f350_amb.city
{
	accessories[]: .f350_amb.chassis
	accessories[]: .f350_amb.fwheel
	accessories[]: .f350_amb.rwheel

	max_speed: 160
	vehicle_mass: 2000

	sound_move[]: "/sound/traffic/car_v8_2.soundref"
	sound_horn[]: "/sound/traffic/car_horn_4.soundref"

	prefer_variant: true 
	variant[]: traffic.f350_amb.id_cda
	variant[]: traffic.f350_amb.id_boi
	variant[]: traffic.f350_amb.id_idf
	variant[]: traffic.f350_amb.nv_lve
	

	lod_dist[]: 53
	lod_dist[]: 70
	allow_parked: false
		
	spawn_ratio: 0


@include "drivers_police.sui"
}

vehicle_accessory: .f350_amb.chassis
{
	data_path: "/def/vehicle/ai/f350_ambulance/id.sii"
}

vehicle_wheel_accessory: .f350_amb.fwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/front_17in_cab.sii"
}

vehicle_wheel_accessory: .f350_amb.rwheel
{
	offset: 0
	data_path: "/def/vehicle/ai_wheel/rear_17in_cab.sii"
}



