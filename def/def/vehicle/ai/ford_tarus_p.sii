traffic_vehicle : traffic.ford_tarus_p
{
	accessories[]: .ford_tarus_p.chassis
	accessories[]: .ford_tarus_p.fwheel
	accessories[]: .ford_tarus_p.rwheel

	max_speed: 160

	lod_dist[]: 53
	lod_dist[]: 70
	
	vehicle_mass: 1700

	prefer_variant: true
	variant[]: traffic.ford_tarus_p.las_vegas
	variant[]: traffic.ford_tarus_p.phoenix
	variant[]: traffic.ford_tarus_p.reno
	
@include "drivers_police.sui"

}
vehicle_accessory: .ford_tarus_p.chassis
{
	data_path: "/def/vehicle/ai/ford_taurus/chassis_police.sii"
}
vehicle_wheel_accessory: .ford_tarus_p.fwheel
{
	data_path: "/def/vehicle/ai_wheel/front_18in_p.sii"
}
vehicle_wheel_accessory: .ford_tarus_p.rwheel
{
	data_path: "/def/vehicle/ai_wheel/rear_18in_p.sii"
}



