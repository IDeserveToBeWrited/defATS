{
	accessories[]: .mag_3l.h_helicopter.tchassis
	accessories[]: .mag_3l.h_helicopter.trwheel0
	accessories[]: .mag_3l.h_helicopter.trwheel1
	accessories[]: .mag_3l.h_helicopter.trwheel2
	
	slave_trailer: traffic.trailer.mag_3l_t.h_helicopter

}

