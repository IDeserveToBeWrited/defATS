traffic_trailer : traffic.trailer.scs_ref_45 # rear
{

	accessories[]: .scs_ref_45.chassis
	accessories[]: .scs_ref_45.rwheel
	accessories[]: .scs_ref_45.rwheel2
	accessories[]: .scs_ref_45.paint_job

	spawn_ratio: 2

	company_paint_job: true

	variant[]: .traffic.trailer.scs_ref_45.front # front
}

vehicle_accessory: .scs_ref_45.chassis
{
	data_path: "/def/vehicle/trailer/scs_box/ref_45_r.sii"
}

vehicle_wheel_accessory: .scs_ref_45.rwheel
{
	offset: 0
	data_path: "/def/vehicle/t_wheel/single.sii"
}

