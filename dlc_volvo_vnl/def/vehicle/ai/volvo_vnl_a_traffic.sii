traffic_vehicle : traffic.volvo.vnl.a
{
	accessories[]: .volvo.vnl.a.chassis
	accessories[]: .volvo.vnl.a.fwheel
	accessories[]: .volvo.vnl.a.rwheel
	accessories[]: .volvo.vnl.a.rwheel2
	accessories[]: .volvo.vnl.a.trlr_cables


	max_speed: 110
	trailer_chains[]: "semi_trailer"

	lod_dist[]: 70
	lod_dist[]: 100

	engine_torque: 1830
}


vehicle_accessory : .volvo.vnl.a.chassis {
	data_path: "/def/vehicle/ai/truck/volvo.vnl/cabin_duty.sii"
}
vehicle_wheel_accessory : .volvo.vnl.a.fwheel {
	data_path: "/def/vehicle/f_wheel/steel.sii"
}
vehicle_wheel_accessory : .volvo.vnl.a.rwheel {
	data_path: "/def/vehicle/r_wheel/steel.sii"
}
vehicle_wheel_accessory : .volvo.vnl.a.rwheel2 {
	data_path: "/def/vehicle/r_wheel/steel.sii"
	offset: 2
}
vehicle_addon_accessory : .volvo.vnl.a.trlr_cables {
	data_path: "/def/vehicle/ai/truck/volvo.vnl/accessory/trlr_cables/stock.sii"
}

