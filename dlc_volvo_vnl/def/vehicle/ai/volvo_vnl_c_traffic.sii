	accessories[]: .volvo.vnl.c.fwheel
	accessories[]: .volvo.vnl.c.rwheel
	accessories[]: .volvo.vnl.c.rwheel2
	accessories[]: .volvo.vnl.c.trlr_cables



	max_speed: 128
	trailer_chains[]: "semi_trailer"

	lod_dist[]: 70
